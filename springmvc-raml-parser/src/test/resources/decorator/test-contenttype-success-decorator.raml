#%RAML 0.8
title: myapi
baseUri: /
version: 1

/contentTypeBase:
  get:
    responses:
      200:
        body:
          application/json:
            schema: |
              {
                "type": "object",
                "$schema": "http://json-schema.org/draft-03/schema",
                "id": "http://jsonschema.net",
                "required": true
              }
  /endpointWithResponseType:
    post:      
      responses:
        200:
          body:
            application/json:
              schema: NamedResponseType
schemas:
  - NamedResponseType: |
      {
        "type": "object",
        "$schema": "http://json-schema.org/draft-03/schema",
        "id": "http://jsonschema.net",
        "required": true
      }